# Uncomment this line to define a global platform for your project
# platform :ios, '9.0'

target 'swift-multithreading-lab' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for swift-blackjack-lab
  def testing_pods
    pod 'Quick', '~> 0.9'
    pod 'Nimble', '~> 4.1'
  end

  target 'swift-multithreading-labTests' do
    inherit! :search_paths
    # Pods for testing
    testing_pods
  end

  target 'swift-multithreading-labUITests' do
    inherit! :search_paths
    # Pods for testing
    testing_pods
  end

end
